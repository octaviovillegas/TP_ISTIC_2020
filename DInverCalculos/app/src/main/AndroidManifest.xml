<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dinvercalculos">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Volumen"></activity>
        <activity android:name=".Calculos" />
        <activity android:name=".MisClaves" />
        <activity android:name=".ActividadBitcoin" />
        <activity android:name=".MercadoInteligente" />
        <activity android:name=".ActividadDollar" />
        <activity android:name=".Menu" />
        <activity android:name=".PreApp" />
        <activity android:name=".Registro" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>