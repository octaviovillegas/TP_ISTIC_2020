    package="com.example.listadecompras">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BuscarProductoLista"></activity>
        <activity android:name=".AboutMe" />
        <activity android:name=".DetalleProducto" />
        <activity android:name=".CargaProducto" />
        <activity android:name=".ListViewProductos" />
        <activity android:name=".Registro" />
        <activity android:name=".MenuPrincipal" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>